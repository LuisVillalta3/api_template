{
  "name": "api_template",
  "version": "1.0.0",
  "description": "Api template with postgres, sequelize and express",
  "main": "src/index.mjs",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts ",
    "lint:fix": "eslint . --ext .ts --fix",
    "make:migration": "sequelize migration:generate --name",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "make:model": "sequelize model:generate --name",
    "make:seed": "sequelize seed:generate --name",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "db:setup": "yarn db:create && yarn db:migrate && yarn db:seed:all",
    "swagger": "swagger-merger -i ./src/docs/swagger/index.yaml -o ./src/docs/swagger.yaml"
  },
  "keywords": [
    "express",
    "sequelize",
    "postgres",
    "swagger",
    "nodejs"
  ],
  "author": "Luis Villalta",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-merger": "^1.5.4",
    "swagger-ui-express": "^4.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/ajv": "^1.0.0",
    "@types/ajv-errors": "^2.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.42",
    "@types/pg": "^8.6.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.1",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
